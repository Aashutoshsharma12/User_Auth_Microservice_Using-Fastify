{
  "name": "fastify_fliz_nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/index.ts --env development",
    "prod": "ts-node src/index.ts --env production",
    "build:dev": "npm run remove-previous-build && tsc --build tsconfig.prod.json && tsc-alias -p tsconfig.prod.json && npm run copy-pre-start && npm run copy-public",
    "build:prod": "tsc --build tsconfig.prod.json && tsc-alias -p tsconfig.prod.json && npm run copy-public && npm run copy-pre-start",
    "remove-previous-build": "rm -rf dist/",
    "copy-public": "cp -r src/public dist/public",
    "copy-pre-start": "cp -r src/pre-start/env dist/pre-start/env",
    "start:dev": "nodemon -x ts-node   src/index.ts --env development",
    "start:prod": "nodemon -x ts-node src/index.ts --env production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.4",
    "@fastify/static": "^8.1.0",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@types/joi": "^17.2.2",
    "ajv-errors": "^3.0.0",
    "amqplib": "^0.10.5",
    "argon2": "^0.41.1",
    "axios": "^1.8.4",
    "command-line-args": "^6.0.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fastify-cors": "^6.1.0",
    "fastify-jwt": "^4.2.0",
    "fs-extra": "^11.3.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "moment-timezone": "^0.5.47",
    "mongoose": "^8.10.0",
    "nodemon": "^3.1.9",
    "opossum": "^8.4.0",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@fastify/autoload": "^6.1.0",
    "@types/amqplib": "^0.10.7",
    "@types/command-line-args": "^5.2.3",
    "@types/cookie-parser": "^1.4.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.1",
    "@types/opossum": "^8.1.8",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3"
  }
